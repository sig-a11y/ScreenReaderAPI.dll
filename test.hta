<title>Test application</title>
</head>
<body>
<p>Test application</p>
<script>
var o = new ActiveXObject("ScreenReaderAPI.Interface");
o.SapiEnable(true);
//o.SetCurrentScreenReaderName("Force SAPI5");
var i = 0;
var f = null; f = function () {
o.SayString(++i, true);
o.SayString("You are using " + o.GetCurrentScreenReaderName(), false);
setTimeout(f,3000);
}
f();
</script>
</body>
